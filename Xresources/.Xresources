!*.font:        -*-terminus-*-*-*-*-12-*-*-*-*-*-*-*, xft:FontAwesome:size=12
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.font:         xft:Dejavu Sans Mono:size=9:antialias=true:hinting=true
urxvt.scrollBar:	false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

urxvt.internalBorder: 10

urxvt.letterSpace: -2

! special
*.foreground:   #68615e
*.background:   #e8e8ea
*.cursorColor:  #68615e

! black
*.color0:       #1b1918
*.color8:       #766e6b

! red
*.color1:       #cd2536
*.color9:       #cd2536

! green
*.color2:       #5ab738
*.color10:      #5ab738

! yellow
*.color3:       #d5911a
*.color11:      #d5911a

! blue
*.color4:       #407ee7
*.color12:      #407ee7

! magenta
*.color5:       #6666ea
*.color13:      #6666ea

! cyan
*.color6:       #00ad9c
*.color14:      #00ad9c

! white
*.color7:       #a8a19f
*.color15:      #e8e8ea
