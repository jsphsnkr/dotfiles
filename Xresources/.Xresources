#include "/home/spinks/colours/conifer"

Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.font:         -*-neep-medium-*-semicondensed-*-11-*-*-*-*-*-*-*
*.boldFont:     -*-neep-medium-*-semicondensed-*-11-*-*-*-*-*-*-*

urxvt.scrollBar:	false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

urxvt.internalBorder: 12

! special
*.foreground:   fg
*.background:   bg
*.cursorColor:  blu

! black
*.color0:       bg
*.color8:       fg

! red
*.color1:       red
*.color9:       red

! green
*.color2:       grn
*.color10:      grn

! yellow
*.color3:       grn
*.color11:      grn

! blue
*.color4:       blu
*.color12:      blu

! magenta
*.color5:       blu
*.color13:      blu

! cyan
*.color6:       blu
*.color14:      blu

! white
*.color7:       fg
*.color15:      fg
