*.color10:      #4cac64
! yellow
*.color3:       #ac8b4c
*.color11:      #ab894b
! blue
*.color4:       #4c64ac
*.color12:      #4c64ac
! magenta
*.color5:       #a04bab
*.color13:      #a04bab
! cyan
