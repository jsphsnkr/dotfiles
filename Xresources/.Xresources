!*.font:		xft: Liberation Mono:pixelsize=12 

*.font:         xft:Terminus:pixelsize=12

urxvt.scrollBar:	false
!urxvt.letterSpace:	-2

Xft.rgba:       none

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! special
*.foreground:   #e6e1dc
*.background:   #2b2b2b
*.cursorColor:  #e6e1dc

! black
*.color0:       #2b2b2b
*.color8:       #666666

! red
*.color1:       #cccccc
*.color9:       #cccccc

! green
*.color2:       #a5c261
*.color10:      #a5c261

! yellow
*.color3:       #f7a251
*.color11:      #f7a251

! blue
*.color4:       #6d9cbe
*.color12:      #6d9cbe

! magenta
*.color5:       #b6b3eb
*.color13:      #b6b3eb

! cyan
*.color6:       #d47dac
*.color14:      #d47dac

! white
*.color7:       #e6e1dc
*.color15:      #f9f7f3

