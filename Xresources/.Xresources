#include "/home/spinks/colours/leaf"

Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.font:         -*-fixed-*-*-*-*-12-*-*-*-*-*-*-*
*.boldFont:     -*-fixed-*-*-*-*-12-*-*-*-*-*-*-*

urxvt.scrollBar:	false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

urxvt.internalBorder: 4

! special
*.foreground:   fg
*.background:   bg
*.cursorColor:  cyn

! black
*.color0:       blk
*.color8:       bblk

! red
*.color1:       red
*.color9:       red

! green
*.color2:       grn
*.color10:      grn

! yellow
*.color3:       ylw
*.color11:      ylw

! blue
*.color4:       blu
*.color12:      blu

! magenta
*.color5:       mag
*.color13:      mag

! cyan
*.color6:       cyn
*.color14:      cyn

! white
*.color7:       wht
*.color15:      wht
