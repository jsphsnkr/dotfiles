Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.font:         xft:Envy Code R:size=9:antialias=true:hinting=true

!*.font:         -*-unifont-*-*-*-*-16-*-*-*-*-*-*-*

urxvt.scrollBar:	false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

urxvt.internalBorder: 10

urxvt.letterSpace: -1

*.foreground:   #545855
*.background:   #e8e8ea
*.cursorColor:  #545855
